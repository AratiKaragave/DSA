def insertion(list):
   for i in range (1,len(list)):
        current_no = list[i]
        pos = i
        while current_no < list[pos-1] and pos > 0:
            list[pos] = list[pos-1]
            pos = pos-1
        list[pos] = current_no
list = [4,5,1,3,2]
insertion(list)
print(list)
